{
  "manifest_version": 3,
  "name": "Boxy",
  "version": "1.0",
  "description": "Play Boxy game, It's so fun.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_title": "Click me to open Boxy game",
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
